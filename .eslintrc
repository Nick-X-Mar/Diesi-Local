{
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "es6": true
  },
  "extends": "react-app",
  "plugins": ["prettier"],
  "rules": {
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": ["Link"],
        "specialLink": ["to"]
      }
    ],
    "prettier/prettier": "error"
  }
}
